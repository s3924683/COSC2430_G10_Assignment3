                    <form class="mb-3 price-input-form">
                      <div class="form-control d-flex">
                        <span>0$</span>
                        <input type="range" class="form-range" min="0" max="10000" value="0">
                        <span>10000$</span>
                      </div>
                    </form>
                  </div>
                  <div class="brand p-2">
                  </div>
